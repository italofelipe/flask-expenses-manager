      ]
    },
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"João Silva\",\n  \"email\": \"joao.silva@email.com\",\n  \"password\": \"MinhaSenha@123\"\n}"
            },
            "url": "{{base_url}}/auth/register",
            "description": "Register a new user. Password must have at least 10 characters, one uppercase letter, one number, and one symbol."
          }
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    var token = jsonData.token || (jsonData.data && jsonData.data.token);",
                  "    if (token) {",
                  "        pm.collectionVariables.set('auth_token', token);",
                  "    }",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"joao.silva@email.com\",\n  \"password\": \"MinhaSenha@123\"\n}"
            },
            "url": "{{base_url}}/auth/login",
            "description": "Authenticate with email (or name) and password. Returns a JWT token. The test script automatically saves the token to the `auth_token` collection variable."
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [],
            "url": "{{base_url}}/auth/logout",
            "description": "Logout the authenticated user. Requires JWT Bearer token."
          }
        }
      ]
    },
